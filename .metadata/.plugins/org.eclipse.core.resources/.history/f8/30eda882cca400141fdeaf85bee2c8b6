#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <signal.h>
#include <errno.h>

int main(void) {
	pid_t pid;

	pid = fork();

	if (pid < 0) {
		perror("Fork failed");
		exit(errno);
	} else if (pid == 0) { //Child

	} else { //Parent

	}

	return EXIT_SUCCESS;
}
